{
    "name": "kenarkose/chronicle",
    "description": "User activity logging for Laravel 5",
    "license": "MIT",
    "keywords": [
        "kenarkose",
        "chronicle",
        "activity",
        "log",
        "laravel",
        "laravel5"
    ],
    "version": "1.3.1",
    "authors": [
        {
            "name": "Hyleeh",
            "email": "egemen@kenarkose.com",
            "homepage": "http://egemenkizilcan.com",
            "role": "Developer / Designer"
        }
    ],
	"require": {
        "php": ">=5.5.9"
	},
    "require-dev": {
        "orchestra/testbench": "~3.1",
        "phpunit/phpunit": "~4.0"
    },
	"autoload": {
		"psr-4": {
            "Kenarkose\\Chronicle\\": "src/"
		},
        "files": [
            "src/helpers.php"
        ]
	},
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "suggest": {
        "laravel/framework": "This package requires Laravel 5."
    }
}
